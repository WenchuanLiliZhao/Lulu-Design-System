{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-color",
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-width: none"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ]
  }
}